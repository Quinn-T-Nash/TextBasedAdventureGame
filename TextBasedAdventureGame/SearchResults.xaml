<Window x:Class="TextBasedAdventureGame.SearchResults"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TextBasedAdventureGame"
        mc:Ignorable="d"
        Title="SearchResults" Height="234" Width="495">
    <Grid Margin="0,0,0,2">
        <Button x:Name="btnLeave" Content="Leave" HorizontalAlignment="Left" Margin="178,21,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.141,0.563" Click="btnLeave_Click"/>
        <Button x:Name="btnTake" Content="Take" HorizontalAlignment="Left" Margin="178,133,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.076,-0.538" Width="34" Click="btnTake_Click"/>
        <Label x:Name="lblFound" Content="Label" HorizontalAlignment="Left" Margin="119,82,0,0" VerticalAlignment="Top" Width="349"/>
    </Grid>
</Window>
